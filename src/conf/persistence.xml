<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AppWebPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>modelo.Entity.Asociacion</class>
    <class>modelo.Entity.Superadministrador</class>
    <class>modelo.Entity.Comuna</class>
    <class>modelo.Entity.Notas</class>
    <class>modelo.Entity.Documentacion</class>
    <class>modelo.Entity.Empleador</class>
    <class>modelo.Entity.Usuario</class>
    <class>modelo.Entity.Carrera</class>
    <class>modelo.Entity.Sede</class>
    <class>modelo.Entity.Practica</class>
    <class>modelo.Entity.Tipopractica</class>
    <class>modelo.Entity.Tipousuario</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:XE"/>
      <property name="javax.persistence.jdbc.password" value="duoc"/>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
      <property name="javax.persistence.jdbc.user" value="DuocPracticas"/>
    </properties>
  </persistence-unit>
</persistence>
